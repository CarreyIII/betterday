<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="me.freelee.betterday.util.IBaseMapper" />
        </plugin>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!--去除自动生成的注释-->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接信息-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="" userId="" password=""/>
        <!--自动生成模型的位置-->
        <javaModelGenerator targetPackage="me.freelee.betterday.model" targetProject="src/main/java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--生成mapper映射文件的位置-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources"/>
        <!--生成DAO的位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="me.freelee.betterday.dao" targetProject="src/main/java"/>
